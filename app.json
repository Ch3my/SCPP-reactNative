{
  "expo": {
    "name": "SCPP",
    "slug": "SCPP-reactNative",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.chemy.scpp"
    },
    "jsEngine": "hermes",
    "extra": {
      "eas": {
        "projectId": "a45ad516-9458-4750-8e82-b0bf836d3454"
      }
    }
  }
}
